# Copy to .env and fill in values

# Neon Postgres
VITE_NEON_DATABASE_URL=postgres://user:pass@host:5432/dbname

# Cloudflare R2
# Do NOT put secrets in VITE_ variables. Configure these on Vercel Project Settings instead:
# R2_ACCOUNT_ID, R2_BUCKET_NAME, R2_ACCESS_KEY_ID, R2_SECRET_ACCESS_KEY

# Optional: external signer URL for local dev fallback (e.g., Cloudflare Worker)
VITE_R2_SIGNER_URL=
